<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta content="width=device-width, initial-scale=1.0" name="viewport">
        <meta content="ie=edge" http-equiv="X-UA-Compatible">
        <title>Document</title>
        {% block head %}{% endblock %}
    </head>
    <body>
        {% spaceless %}
            <nav class="navbar navbar-expand-lg fixed-top px-md-0 navbar-initial">
                <div class="container">
                    <a class="navbar-brand" href="#">Navbar</a>
                    <button aria-controls="navbarToggler" aria-expanded="false" aria-label="Toggle navigation" class="navbar-toggler" data-target="#navbarToggler" data-toggle="collapse" type="button">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="collapse navbar-collapse justify-content-end" id="navbarToggler">
                        <ul class="navbar-nav">
                            {% for item in navbar %}
                                {% if item.dropdown is defined %}
                                    <li class="nav-item dropdown px-3">
                                        <a aria-expanded="false" aria-haspopup="true" class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" id="navbarDropdown" role="button">
                                            {{ item.name }}
                                        </a>
                                        <div aria-labelledby="navbarDropdown" class="dropdown-menu">
                                            {% for subitem in item.dropdown %}
                                                <a class="dropdown-item" href="#">{{ subitem.name }}</a>
                                            {% endfor %}
                                        </div>
                                    </li>
                                {% else %}
                                    <li class="nav-item px-3">
                                        <a class="nav-link" href="#">{{ item.name }}</a>
                                    </li>
                                {% endif %}

                            {% endfor %}
                        </ul>
                    </div>
                </div>
            </nav>
            {% block body %}{% endblock %}
            {% block script %}{% endblock %}
            <script async defer src="./dist/fonts.js?v=190328-1510"></script>
            <script defer src="./dist/styles.js?v=190530-0528"></script>
            <script defer src="./dist/images.js?v=190530-0528"></script>
            <script defer src="./dist/scripts.js?v=190530-0528"></script>
        {% endspaceless %}
    </body>
</html>
